{
	"name": "salesforce-object-browser",
	"displayName": "SalesForce Object Browser",
	"description": "",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.35.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			
			{
				"command": "Extension.helloWorld",
      			"title": "Refresh",
      			"icon": {
        		"light": "resources/light/refresh.svg",
        		"dark": "resources/dark/refresh.svg"
				  }
			}
		],
		"viewsContainers": {
			"activitybar": [
			  {
				"id": "object-explorer",
				"title": "Object Explorer",
				"icon": "media/dep.svg"
			  }
			]
		  },
		"views": {
			"object-explorer": [
			  {
				"id": "objects",
				"name": "Objects"
			  }
			]
		  },
		  "menus": {
			"view/title": [
			  {
				"command": "Extension.helloWorld",
				"when": "view == objects",
				"group": "navigation"
			  }
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "npm run compile && node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"typescript": "^3.3.1",
		"vscode": "^1.1.28",
		"tslint": "^5.12.1",
		"@types/node": "^10.12.21",
		"@types/mocha": "^2.2.42"
	}
}
